{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "github>murn0/renovate-config"
  ],
  "regexManagers": [
    {
      "fileMatch": ["install-aquaproj\\.mk$"],
      "matchStrings": ["raw\\.githubusercontent\\.com/aquaproj/aqua-installer/(?<currentValue>[^'\" \\n]+)/aqua-installer\n\t@echo \"(?<currentDigest>[0-9a-f]*)?  aqua-installer\""],
      "depNameTemplate": "aquaproj/aqua-installer",
      "datasourceTemplate": "github-releases"
    }
  ]
}
