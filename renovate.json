{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "github>murn0/renovate-config"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^README.md$"],
      "matchStrings": ["(https://github.com/(?<depName>(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)) `(?<currentValue>[^'\" \\n]+)`"]
    }
  ]
}
